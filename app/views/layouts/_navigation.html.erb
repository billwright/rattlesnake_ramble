        <div class="dropdown">
          <button class="btn btn-warning dropdown-toggle" type="button" id="dropdownMenu1" data-toggle="dropdown" aria-haspopup="true" aria-expanded="true">
            Entrants
            <span class="caret"></span>
          </button>
          <ul class="dropdown-menu" aria-labelledby="dropdownMenu1">
            <li><%= link_to "Full Course", race_edition_path(current_full_course_edition), id: "logo" %></li>
            <li><%= link_to "Kids Course", race_edition_path(current_kids_course_edition), id: "logo" %></li>
          </ul>
        </div>
      </div>
      <div class="nav_item">
        <%= link_to "Results", "https://www.opensplittime.org/organizations/rattlesnake-ramble", class: "btn btn-success", id: "logo" %>
