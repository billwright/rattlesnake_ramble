              <li><%= link_to "View Full Course Emails", racer_emails_race_edition_path(current_full_course_edition), id: "logo" %></li>
              <li><%= link_to "View Kids Course Emails", racer_emails_race_edition_path(current_kids_course_edition), id: "logo" %></li>
              <li class="divider" role="separator"></li>
